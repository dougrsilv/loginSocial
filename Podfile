# platform :ios, '9.0'

target 'DevPoliChallenge' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DevPoliChallenge
    pod 'Firebase'
    pod 'FirebaseAuth'
    pod 'FirebaseStorage'
